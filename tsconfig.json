{
    "include": [
        "./src/",
    ],
    "exclude": [
        "./src/**.test.ts",
    ],
    "compilerOptions": {
        "target": "ES5",
        "lib": [
            "ES2020",
        ],
        "module": "CommonJS",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "outDir": "./.ts/",
        "declaration": true,
        "downlevelIteration": true,
    },
}
